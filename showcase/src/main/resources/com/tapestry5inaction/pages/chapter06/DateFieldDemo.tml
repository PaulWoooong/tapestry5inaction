<html t:type="layout" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd">

    <p>${successMessage}</p>

    <form t:type="form" clientValidation="false">
        <t:errors/>
        <div>
            <label t:type="label" for="birthdayField"/>
            <input t:type="datefield" t:id="birthdayField" value="birthday" validate="required"/>
        </div>

        <div>
            <input type="submit" value="Submit" />
        </div>
    </form>

</html>